{
  "name": "latal-plugins",
  "owner": {
    "name": "Tuáº¥n",
    "url": "https://github.com/lATAl"
  },
  "metadata": {
    "description": "Claude Code plugins by lATAl"
  },
  "plugins": [
    {
      "name": "elixir-lsp",
      "source": "./plugins/elixir-lsp",
      "description": "Elixir LSP support using Expert - the official Elixir Language Server",
      "version": "1.0.1",
      "keywords": ["elixir", "lsp", "expert", "language-server"],
      "strict": false,
      "lspServers": {
        "elixir": {
          "command": "expert",
          "args": ["--stdio"],
          "extensionToLanguage": {
            ".ex": "elixir",
            ".exs": "elixir",
            ".heex": "heex",
            ".eex": "eex",
            ".leex": "eex"
          }
        }
      }
    }
  ]
}
